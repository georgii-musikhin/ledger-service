root = true

[*]
charset = utf-8
indent_size = 2
indent_style = space
insert_final_newline = true
end_of_line = lf

[*.{kt, kts, java}]
max_line_length = 100
# Intellij IDEA specific
ij_kotlin_name_count_to_use_star_import = 999
ij_kotlin_name_count_to_use_star_import_for_members = 999

[*.bat]
end_of_line = crlf
